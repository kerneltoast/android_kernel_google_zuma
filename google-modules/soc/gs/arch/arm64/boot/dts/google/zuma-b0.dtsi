	};
};


&dsu_lat {
		/* CL0         DSU */
	core-dev-table-cl0-low-latency-v2 =
		<  324000        324000
		   610000        610000
		   820000        820000
		   955000        970000
		  1098000       1098000
		  1197000       1197000 >;

		/* CL1         DSU */
	core-dev-table-cl1-low-latency-v2 =
		<  402000        324000
		   578000        610000
		   910000        820000
		  1065000        970000
		  1836000       1098000
		  1945000       1197000
		  2130000       1444000 >;

		/* CL2         DSU */
	core-dev-table-cl2-low-latency-v2 =
		<  500000        324000
		   880000        610000
		  1164000        820000
		  1298000        970000
		  1885000       1098000
		  2147000       1197000
		  2363000       1444000
		  2850000       1704000 >;

		   /* CL0         DSU */
	core-dev-table-cl0-base-v2 =
		<  324000        324000
		  1098000        610000 >;

		   /* CL1         DSU */
	core-dev-table-cl1-base-v2 =
		<  402000       324000
		  1418000       610000
		  1836000       970000
		  2130000      1098000 >;

		   /* CL2         DSU */
	core-dev-table-cl2-base-v2 =
		<  500000       324000
		  1298000       610000
		  2049000       970000
		  2363000      1098000
		  2802000      1444000 >;

	dsu-bci-table-v2 =
		/* DSU         BCI */
		< 324000        315000
		610000        575000
		820000        840000
		970000        1050000
		1197000       1205000
		1548000       1401000 >;
};

