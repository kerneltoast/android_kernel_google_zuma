};

&bigwave {
	ip_ver = <0x1>;
};

&gs_dsulat {
	/* Linearly Dependent Domains. */
	secondary_domains {
		bci_freq {
			devfreq_domain_name = "DSU2BCI";
			pm_qos_class = <PM_QOS_BCI_THROUGHPUT>;
			core-dev-table-latency =
			/* DSU     BCI */
			< 324000  315000 >,
			< 610000  575000 >,
			< 820000  840000 >,
			< 970000  1050000 >,
			< 1197000 1205000 >,
			< 1548000 1401000 >;
		};
	};

	dsulat_governor {
		gs_dsulat_cl0 {
			cpulist = <&cpu0>, <&cpu1>, <&cpu2>, <&cpu3>;
			ratio_ceil = <700>;
			stall_floor = <1750>;
			cpuidle_state_depth_threshold = <2>;

			core-dev-table-latency =
			/* CL0   DSU */
			< 324000  324000 >,
			< 610000  610000 >,
			< 820000  820000 >,
			< 955000  970000 >,
			< 1098000 1098000 >,
			< 1197000 1197000 >;

			core-dev-table-base =
			/* CL0    DSU */
			< 324000  324000 >,
			< 1098000 610000 >;
		};
		gs_dsulat_cl1 {
			cpulist = <&cpu4>, <&cpu5>, <&cpu6>, <&cpu7>;
			ratio_ceil = <1000>;
			stall_floor = <1750>;
			cpuidle_state_depth_threshold = <2>;

			core-dev-table-latency =
			/* CL1    DSU */
			< 402000  324000 >,
			< 578000  610000 >,
			< 910000  820000 >,
			< 1065000 970000 >,
			< 1836000 1098000 >,
			< 1945000 1197000 >,
			< 2130000 1444000 >;

			core-dev-table-base =
			/* CL1    DSU */
			< 402000  324000 >,
			< 1418000 610000 >,
			< 1836000 970000 >,
			< 2130000 1098000 >;

		};
		gs_dsulat_cl2 {
			cpulist = <&cpu8>;
			ratio_ceil = <3000>;
			stall_floor = <1750>;
			cpuidle_state_depth_threshold = <2>;

			core-dev-table-latency =
			/* CL2    DSU */
			< 500000  324000 >,
			< 880000  610000 >,
			< 1164000 820000 >,
			< 1298000 970000 >,
			< 1885000 1098000 >,
			< 2147000 1197000 >,
			< 2363000 1444000 >,
			< 2850000 1704000 >;

			core-dev-table-base =
			/* CL2    DSU */
			< 500000       324000 >,
			< 1298000      610000 >,
			< 2049000      970000 >,
			< 2363000      1098000 >,
			< 2802000      1444000 >;
		};
	};
};

