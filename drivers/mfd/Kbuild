obj-$(CONFIG_MFD_S2MPG14) 		+= s2mpg14-mfd.o
s2mpg14-mfd-$(CONFIG_MFD_S2MPG14) 	+= s2mpg14-core.o s2mpg14-irq.o
obj-$(CONFIG_MFD_S2MPG15) 		+= s2mpg15-mfd.o
s2mpg15-mfd-$(CONFIG_MFD_S2MPG15) 	+= s2mpg15-core.o s2mpg15-notifier.o
